{
    "name": "politriukas/avataaars",
    "type": "library",
    "description": "PHP SVG avatar generator based on https://github.com/fangpenlin/avataaars-generator",
    "keywords": ["avatars", "avatar generator", "avataaars", "avataaars generator", "getavataaars", "svg avatar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas NorkÅ«nas",
            "email": "norkunas.tom@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "meyfa/php-svg": "^0.14"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.9",
        "phpstan/phpstan": "^1.9.7",
        "phpstan/phpstan-deprecation-rules": "^1.1.1",
        "phpstan/phpstan-phpunit": "^1.3.3",
        "phpstan/phpstan-strict-rules": "^1.4.4",
        "phpunit/phpunit": "^9.5.27"
    },
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": { "Politriukas\\Avataaars\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Politriukas\\Avataaars\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
